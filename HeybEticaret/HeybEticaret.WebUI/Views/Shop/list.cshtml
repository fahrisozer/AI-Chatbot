@model ProductListModel

<div class="row mt-5">
    <div class="col-md-3">
        @await Component.InvokeAsync("CategoryList")
    </div>
    <div class="col-md-9">

        <div class="row mt-5">
            @foreach (var product in Model.Products)
            {
                <partial name="_product" for="@product" />
            }
        </div>

        <div page-model="@Model.PagingInfo"></div>

    </div>
</div>